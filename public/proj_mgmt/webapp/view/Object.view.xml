<core:View xmlns:core="sap.ui.core"
           xmlns="sap.m"
           controllerName="mycompany.myapp.MyWorklistApp.controller.Object"
           height="100%">
  <Page
      id="page"
      busy="{objectView>/busy}"
		  busyIndicatorDelay="{objectView>/delay}"
      showHeader="true">
		<customHeader>
			<Bar>
				<contentRight>
					<Button id="sync"   text="Sync"   press="onSyncPress" visible="false"/>
					<Button id="edit"   text="Edit"   press="onEditPress" />
					<Button id="save"   text="Save"   press="onSavePress" />
					<Button id="cancel" text="Cancel" press="onCancelPress" />
				</contentRight>
			</Bar>
		</customHeader>
  </Page>
</core:View>
